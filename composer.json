      "Adaptcms\\FieldText\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adaptcms\\FieldText\\FieldTextServiceProvider"
      ],
      "aliases": {
        "FieldText": "Adaptcms\\FieldText\\Facades\\FieldText"
      }
    }
