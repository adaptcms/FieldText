    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^5.1",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Adaptcms\\FieldText\\": "src/"
